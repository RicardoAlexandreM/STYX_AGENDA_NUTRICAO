SELECT *
  FROM NUTRITIONIST N
  JOIN NUTRITIONIST_CLIENTS NC ON NC.ID_NUTRITIONIST = N.ID_NUTRITIONIST
  JOIN CLIENTS C ON C.ID_CLIENT = NC.ID_CLIENTS
 WHERE N.ID_NUTRITIONIST = 1
